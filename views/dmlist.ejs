                  <tbody>
                    <%
                         for(var i=leng-((page*page_num)-page_num); i>leng-(page*page_num); i--)
                         {
                           if(i<0){
                             break;
                           }else{
                               var oneItem = rows[i];
                          %>
                          <tr>
                            <td><%=oneItem.idx%></td>
                            <td><%=oneItem.zzlat%></td>
                            <td><%=oneItem.zzlon%></a></td>
                            <td><%=oneItem.location%></td>
                            <td><%=oneItem.movie%></td>
                            <td align="center">
                            <a href='/mapboard/delete/<%=oneItem.idx%>' class="btn btn-danger"><em class="fa fa-trash"></em></a>
                            </td>
                            <td align="center"><a href="http://map.daum.net/link/to/tim여행지,<%=oneItem.zzlat%>,<%=oneItem.zzlon%>" class="btn btn-info"  target="_blank">길찾기</a>

